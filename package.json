{
  "name": "Skinn-react-test",
  "version": "1.0.0",
  "keywords": [
    "util",
    "functional",
    "server",
    "client",
    "browser"
  ],
  "author": "David Houthoofdt",
  "contributors": [],
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "plugins": [
      "react"
    ],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "extends": "eslint:recommended",
    "env": {
      "browser": true,
      "jquery": true,
      "node": true,
      "es6": true
    },
    "rules": {
      "react/jsx-uses-react": 1,
      "react/jsx-uses-vars": 1,
      "no-console": 0,
      "no-useless-escape": 0,
      "quotes": [
        "warn",
        "single"
      ]
    }
  },
  "dependencies": {
    "babel-eslint": "~7.2.0",
    "babel-jest": "^20.0.0",
    "browserify": "~14.4.0",
    "eslint": "^3.19.0",
    "eslint-plugin-react": "^7.0.1",
    "fbemitter": "^2.1.1",
    "jquery": "~2.1.0",
    "lodash": "^4.17.4",
    "react": "~15.5.4",
    "react-bootstrap": "~0.31.0",
    "react-dom": "~15.5.4",
    "react-slider": "~0.8.0",
    "requirejs": "~2.3.3",
    "uglify-js": ">=3.0.15"
  },
  "devDependencies": {
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^4.0.0",
    "gulp-babel": "^6.1.2",
    "gulp-clean": "^0.3.2",
    "gulp-clean-css": "^3.4.2",
    "gulp-concat": "^2.6.1",
    "gulp-load-plugins": "^1.5.0",
    "gulp-minify": "^1.0.0",
    "gulp-sourcemaps": "^2.6.0",
    "gulp-uglify": "^3.0.0",
    "webpack": "^3.8.1"
  }
}
